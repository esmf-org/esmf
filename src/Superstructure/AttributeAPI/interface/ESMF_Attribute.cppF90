      endif
      else if (directional == 3) then
      if (writeflag%value .eq. ESMF_ATTWRITE_TAB%value) then
        call c_ESMC_AttributeWriteTab(field%ftypep%base, convention, &
          purpose, fobject, ftarobj, localrc)
