     call c_ESMC_AttPackCreate(array, name1, fconvention, &
        fpurpose, fobject, attpacknest, localrc)
      call c_ESMC_AttPackCreate(array, name2, fconvention, &
        fpurpose, fobject, attpacknest, localrc)
      call c_ESMC_AttPackCreate(array, name3, fconvention, &
