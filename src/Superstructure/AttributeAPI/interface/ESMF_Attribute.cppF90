@\
^undef  ESMF_METHOD @\
^define ESMF_METHOD "ESMF_AttGetAttPack" @\
 @\
!BOPI @\
! !IROUTINE: ESMF_AttributeGetAttPack - Retrieve a handle to an Attribute package @\
! @\
! !INTERFACE: @\
      ! Private name; call using ESMF_AttributeGetAttPack() @\
      subroutine mclass##AttGetAttPack(mthis, & @\
