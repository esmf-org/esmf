      if (len(name) > ESMF_MAXSTR) then @\
        call ESMF_LogSetError(rcToCheck=ESMF_RC_NOT_VALID, & @\
          msg="len(name) cannot be larger than ESMF_MAXSTR for now", & @\
          ESMF_CONTEXT, rcToReturn=rc) @\
