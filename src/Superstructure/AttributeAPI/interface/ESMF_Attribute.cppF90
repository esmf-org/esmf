        itemCount, isPresent, rc) @\
! @\
! !ARGUMENTS: @\
      type(mclass), intent(in) :: mthis   @\
      character (len = *), intent(in) :: name @\
      type(ESMF_AttPack), intent(inout) :: attpack @\
      character (len=*), dimension(:), intent(out) :: valueList @\
