    type(ESMF_GeomBase)   :: geombase @\
    type(ESMF_FieldStatus_Flag)    :: status @\
@\
    if (present(rc)) then @\
      rc = ESMF_RC_NOT_IMPL @\
    endif @\
    localrc = ESMF_RC_NOT_IMPL @\
@\
